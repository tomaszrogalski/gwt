<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">

	<g:HTMLPanel>
		<h1>DODAJ KLIENTA</h1>
		<hr />

		<g:HorizontalPanel>
			<g:Label text="IMIE" />
			<g:TextBox ui:field="textBoxImie" />
		</g:HorizontalPanel>

		<g:HorizontalPanel>
			<g:Label text="NAZWISKO" />
			<g:TextBox ui:field="textBoxNazwisko" />
		</g:HorizontalPanel>

		<g:HorizontalPanel>
			<g:Label text="ULICA" />
			<g:TextBox ui:field="textBoxUlica" />
		</g:HorizontalPanel>

		<g:HorizontalPanel>
			<g:Label text="NR DOMU" />
			<g:TextBox ui:field="textBoxNrDomu" />
		</g:HorizontalPanel>

		<g:HorizontalPanel>
			<g:Label text="MIEJSCOWOSC" />
			<g:TextBox ui:field="textBoxMiejscowosc" />
		</g:HorizontalPanel>

		<g:HorizontalPanel>
			<g:Label text="KOD POCZTOWY" />
			<g:TextBox ui:field="textBoxKodPocztowy" />
		</g:HorizontalPanel>

		<g:HorizontalPanel>
			<g:Label text="" ui:field="errorLabel" />
		</g:HorizontalPanel>
		<hr />

		<g:HorizontalPanel>
			<g:Button text="DODAJ" ui:field="buttonDodaj" />
		</g:HorizontalPanel>
		<br />

	</g:HTMLPanel>
</ui:UiBinder>