<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<g:HTMLPanel>
		<h1>DODAJ USLUGE</h1>
		<hr />

		<g:HorizontalPanel>
			<g:Label text="NAZWA" />
			<g:TextBox ui:field="textBoxNazwa" />
		</g:HorizontalPanel>

		<g:HorizontalPanel>
			<g:Label text="CENA/H" />
			<g:DoubleBox ui:field="doubleBoxCenaZaGodzine" />
			<g:Label text=" ZŁ/GODZINE" />
		</g:HorizontalPanel>

		<g:HorizontalPanel>
			<g:Label text="VAT" />
			<g:DoubleBox ui:field="doubleBoxVAT" />
			<g:Label text=" %" />
		</g:HorizontalPanel>
		<g:HorizontalPanel>
			<g:Label text="" ui:field="errorLabel" />
		</g:HorizontalPanel>
		<hr />

		<g:HorizontalPanel>
			<g:Button text="DODAJ" ui:field="buttonDodaj" />
		</g:HorizontalPanel>
		<br />

	</g:HTMLPanel>
</ui:UiBinder> 